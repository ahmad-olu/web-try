{
    "name": "web-try-monorepo",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:saleoff": "pnpm --filter saleoff run dev --open",
        "dev:back:saleoff": "pnpm --filter saleoff-backend run dev",
        "dev:stately": "pnpm --filter stately run dev --open",
        "build": "pnpm -r build"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}